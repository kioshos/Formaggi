<Application x:Class="Formaggi.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Formaggi"
             StartupUri="/View/LoginView.xaml">
    <Application.Resources>
        <BitmapImage x:Key="parmigiano" UriSource="pack://application:,,,/Image/parmigiano.jpg"/>
        <BitmapImage x:Key="gorgonzola" UriSource="pack://application:,,,/Image/gorgonzola.jpg"/>
        <BitmapImage x:Key="mascarpone" UriSource="pack://application:,,,/Image/mascarpone.jpg"/>
        <BitmapImage x:Key="mozzarella" UriSource="pack://application:,,,/Image/mozzarella.jpg"/>
        <BitmapImage x:Key="pecorino" UriSource="pack://application:,,,/Image/pecorino.jpg"/>
        <BitmapImage x:Key="ricotta" UriSource="pack://application:,,,/Image/ricotta.jpg"/>
        <FontFamily x:Key="Ubuntu">pack://application:,,,/StylesUbuntuMono-Regular.ttf#UbuntuMono</FontFamily>
    </Application.Resources>
</Application>
